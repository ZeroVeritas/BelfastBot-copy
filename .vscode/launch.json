{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "YohaneBot (Debug)",
            "type":"coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "${workspaceRoot}/YohaneBot/bin/Debug/netcoreapp3.0/YohaneBot.dll"
            ],
            "cwd": "${workspaceRoot}/YohaneBot/bin/Debug/netcoreapp3.0/",
            "preLaunchTask": "Build YohaneBot (Debug)",
            "linux": {
                "env": {
                    "LD_LIBRARY_PATH": "${workspaceRoot}/YohaneBot/bin/Debug/netcoreapp3.0:${env:LD_LIBRARY_PATH}"
                }
            },
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "name": "YohaneBot (Release)",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "${workspaceRoot}/YohaneBot/bin/Release/netcoreapp3.0/YohaneBot.dll"
            ],
            "cwd": "${workspaceRoot}/YohaneBot/bin/Release/netcoreapp3.0/",
            "preLaunchTask": "Build YohaneBot (Release)",
            "linux": {
                "env": {
                    "LD_LIBRARY_PATH": "${workspaceRoot}/YohaneBot/bin/Release/netcoreapp3.0:${env:LD_LIBRARY_PATH}"
                }
            },
            "logging": {
                "moduleLoad": false,
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ],
}
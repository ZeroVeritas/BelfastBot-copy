{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "YohaneDiscordClient (Debug)",
            "type":"coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "${workspaceRoot}/YohaneDiscordClient/bin/Debug/netcoreapp3.0/YohaneDiscordClient.dll"
            ],
            "cwd": "${workspaceRoot}/YohaneDiscordClient/bin/Debug/netcoreapp3.0/",
            "preLaunchTask": "Build YohaneDiscordClient (Debug)",
            "linux": {
                "env": {
                    "LD_LIBRARY_PATH": "${workspaceRoot}/YohaneDiscordClient/bin/Debug/netcoreapp3.0:${env:LD_LIBRARY_PATH}"
                }
            },
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "name": "YohaneDiscordClient (Release)",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "${workspaceRoot}/YohaneDiscordClient/bin/Release/netcoreapp3.0/YohaneDiscordClient.dll"
            ],
            "cwd": "${workspaceRoot}/YohaneDiscordClient/bin/Release/netcoreapp3.0/",
            "preLaunchTask": "Build YohaneDiscordClient (Release)",
            "linux": {
                "env": {
                    "LD_LIBRARY_PATH": "${workspaceRoot}/YohaneDiscordClient/bin/Release/netcoreapp3.0:${env:LD_LIBRARY_PATH}"
                }
            },
            "logging": {
                "moduleLoad": false,
            }
        },
        {
            "name": "YohaneWebClient (Debug)",
            "type":"coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "${workspaceRoot}/YohaneWebClient/bin/Debug/netcoreapp3.1/YohaneWebClient.dll"
            ],
            "cwd": "${workspaceRoot}/YohaneWebClient/",
            "preLaunchTask": "Build YohaneWebClient (Debug)",
            "linux": {
                "env": {
                    "LD_LIBRARY_PATH": "${workspaceRoot}/YohaneWebClient/bin/Debug/netcoreapp3.1:${env:LD_LIBRARY_PATH}"
                }
            },
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "name": "YohaneWebClient (Release)",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "${workspaceRoot}/YohaneWebClient/bin/Release/netcoreapp3.1/YohaneWebClient.dll"
            ],
            "cwd": "${workspaceRoot}/YohaneWebClient/",
            "preLaunchTask": "Build YohaneWebClient (Release)",
            "linux": {
                "env": {
                    "LD_LIBRARY_PATH": "${workspaceRoot}/YohaneWebClient/bin/Release/netcoreapp3.1:${env:LD_LIBRARY_PATH}"
                }
            },
            "logging": {
                "moduleLoad": false,
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ],
}